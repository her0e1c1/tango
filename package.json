{
  "name": "tango-web",
  "version": "2.0.3",
  "private": true,
  "scripts": {
    "deploy": "firebase deploy --only hosting",
    "login": "firebase login",
    "start": "vite dev",
    "build": "vite build",
    "db": "firebase emulators:start",
    "test": "vitest run --exclude '**/firestore/**/*.spec.ts'",
    "test:db": "vitest run --no-file-parallelism src/action/firestore/ --exclude '**/firestore/rule/*.spec.ts'",
    "test:rule": "vitest run --no-file-parallelism src/action/firestore/rule/",
    "fmt": "prettier './src/**/*.{ts,tsx,js,jsx}' --check",
    "fmt:fix": "prettier './src/**/*.{ts,tsx,js,jsx}' --write",
    "lint": "tsc && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --fix",
    "storybook": "storybook dev",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/katex": "^0.16.7",
    "@types/papaparse": "^5.3.14",
    "autoprefixer": "^10.4.2",
    "classnames": "^2.3.1",
    "file-saver": "^2.0.5",
    "firebase": "^10.1.0",
    "highlight.js": "^11.8.0",
    "katex": "^0.15.2",
    "moment": "^2.29.1",
    "papaparse": "^5.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
    "react-icons": "^5.3.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-swipeable": "^7.0.2",
    "react-use": "^17.3.2",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.1",
    "sass": "^1.64.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.2.23",
    "@firebase/rules-unit-testing": "*",
    "@storybook/addon-essentials": "^8.0.2",
    "@storybook/addon-interactions": "^8.0.2",
    "@storybook/addon-links": "^8.0.2",
    "@storybook/addon-themes": "^8.0.2",
    "@storybook/blocks": "^8.0.2",
    "@storybook/react": "^8.0.2",
    "@storybook/react-vite": "^8.0.2",
    "@storybook/test": "^8.0.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "*",
    "@testing-library/user-event": "*",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.8.0",
    "firebase-tools": "^13.6.0",
    "postcss": "^8.4.38",
    "prettier": "*",
    "storybook": "^8.0.2",
    "storybook-dark-mode": "^4.0.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false,
    "printWidth": 120,
    "useTabs": false
  }
